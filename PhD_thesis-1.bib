@article{Henneaux1985,
author="Henneaux, Marc
and Teitelboim, Claudio",
title="Asymptotically anti-de Sitter spaces",
journal="Communications in Mathematical Physics",
year="1985",
month="Sep",
day="01",
volume="98",
number="3",
pages="391--424",
abstract="Asymptotically anti-de Sitter spaces are defined by boundary conditions on the gravitational field which obey the following criteria: (i) they are O(3, 2) invariant; (ii) they make the O(3, 2) surface integral charges finite; (iii) they include the Kerr-anti-de Sitter metric. An explicit expression of the O(3, 2) charges in terms of the canonical variables is given. These charges are shown to close in the Dirac brackets according to the anti-de Sitter algebra. The results are extended to the case ofN=1 supergravity. The coupling to gravity of a third-rank, completely antisymmetric, abelian gauge field is also considered. That coupling makes it possible to vary the cosmological constant and to compare the various anti-de Sitter spaces which are shown to have the same energy.",
issn="1432-0916",
doi="10.1007/BF01205790",
url="https://doi.org/10.1007/BF01205790"
}


