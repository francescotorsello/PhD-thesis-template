@Article{Davis1965,
author="Davis, W. R.
and Moss, M. K.",
title="Conservation Laws of the general theory of relativity",
journal="Il Nuovo Cimento (1955-1965)",
year="1965",
month="Aug",
day="01",
volume="38",
number="4",
pages="1531--1557",
abstract="A fundamental method of approach to the formulation of the conservation laws of the general theory of relativity is presented. This approach is based on the symmetry properties which are admitted by the specific physical space-times under consideration and which underly the conservation laws in differential form that are shown to follow from a generally covariant generator. This method embraces procedures which permit the proper conservation laws of the field to be formulated in a consistent and well-defined manner. For example, if the total energy of a physical system characterized by a solution of Einstein's field equations is conserved, then this conserved quantity may be formulated in any other co-ordinate system or frame of reference related to the original co-ordinate representation by transformations of the general form -xi=xi (x1, x2, x3, x4). Symmetry properties characterized by groups of motions (Killing vectors) of given space-times are considered in detail. The conservation laws of energy, linear momentum, and angular momentum of the theory of general relativity follow, if admitted, as an immediate consequence of these motions. The mechanical and field conservation laws that follow in consequence of the Riemannian geometry of space-time admitting symmetry properties more general than motions are considered in detail in a second paper, which follows in this issue.",
issn="1827-6121",
doi="10.1007/BF02750077",
url="https://doi.org/10.1007/BF02750077"
}

